function setTab(p: player):
	set {_pref} to prefix of {_p}
	set {_p}'s displayname to "ยง7[%{_pref}%ยง7] ยง6%{_p}%"
	
on join:
	setTab(player)
	
command /refreshtab:
	trigger:
		loop all players:
			setTab(loop-player)
		send "%{dd::f::prefix}%Tabliste neu geladen."
			
on load:
	loadModule("System: Tablist")